{
  "expo": {
    "name": "Tracks",
    "slug": "Tracks",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "React Native Expo application. Makes use of the device location service so that it can create tracks on a map where the user has been walking/cycling etc.",
    "githubUrl": "https://github.com/kirizhu/Tracks"
  }
}
